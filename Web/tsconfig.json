{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "ES2015",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "plugins": [ { "name": "ts-vue-plugin" } ],
    "strict": true,
    "types": [ "webpack-env" ]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
